                <WMControl:WMTabItem.MyEnterBrush>
                    <ImageBrush ImageSource="Skin/Bd/enter.png"/>
                </WMControl:WMTabItem.MyEnterBrush>
                <Grid>
                    <Grid.Background>
                        <ImageBrush ImageSource="Skin/home.png" Stretch="Uniform"/>
                    </Grid.Background>
                </Grid>
            </WMControl:WMTabItem>
            <WMControl:WMTabItem Width="80" Height="75" Foreground="White" Header="木马查杀">
                <WMControl:WMTabItem.Background>
                    <ImageBrush ImageSource="Skin/Icon/ico_dsmain.png"/>
