                </Grid>
            </WMControl:WMTabItem>
            <WMControl:WMTabItem Width="80" Height="75" Foreground="White" Header="漏洞修复">
                <WMControl:WMTabItem.Background>
                    <ImageBrush ImageSource="Skin/Icon/ico_VulRepair.png"/>
