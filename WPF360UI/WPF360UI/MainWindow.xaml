            </WMControl:WMButton.MyEnterBrush>
            <WMControl:WMButton.Background>
                <ImageBrush ImageSource="Skin/Button/msn.png"/>
            </WMControl:WMButton.Background>
        </WMControl:WMButton>
        <WMControl:WMButton HorizontalAlignment="Right" VerticalAlignment="Top" Width="27" Height="22" Margin="0,0,145,0">
            <WMControl:WMButton.MyMoverBrush>
                <ImageBrush ImageSource="Skin/Button/skm.png"/>
            </WMControl:WMButton.MyMoverBrush>
            <WMControl:WMButton.MyEnterBrush>
                <ImageBrush ImageSource="Skin/Button/ske.png"/>
            </WMControl:WMButton.MyEnterBrush>
            <WMControl:WMButton.Background>
                <ImageBrush ImageSource="Skin/Button/skn.png"/>
            </WMControl:WMButton.Background>
        </WMControl:WMButton>
    </Grid>
</Window>
